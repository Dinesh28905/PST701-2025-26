package PST;

import java.util.Scanner;

public class Char_for_given_Ascii
{

    public static void main(String[] args)
    {

        Scanner sc = new Scanner(System.in);

        System.out.print("Enter ASCII value: ");
        int ascii = sc.nextInt();
        printChar(ascii);
       
        sc.close();
    }

  
    static void printChar(int ascii) 
    {
        char ch = (char) ascii;  
        
        System.out.println("Character for ASCII " + ascii + " is: " + ch);
    }
}
